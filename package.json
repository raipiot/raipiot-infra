{
  "name": "raipiot-infra",
  "description": "raipiot front-end infrastructure.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev:react": "turbo dev --filter=react-playground",
    "dev:next": "turbo dev --filter=next-playground",
    "dev:storybook": "turbo dev --filter=storybook",
    "dev:antd": "turbo dev --filter=@raipiot-infra/antd",
    "dev:auto-import": "turbo dev --filter=@raipiot-infra/auto-import",
    "dev:packages": "turbo dev --scope=@raipiot-infra/utils --scope=@raipiot-infra/config",
    "dev:axios": "turbo dev --filter=@raipiot-infra/axios",
    "dev:config": "turbo dev --filter=@raipiot-infra/config",
    "dev:utils": "turbo dev --filter=@raipiot-infra/utils",
    "build:apps": "turbo build --filter=./apps/*",
    "build:react": "turbo build --filter=react-playground",
    "build:next": "turbo build --filter=next-playground",
    "build:storybook": "turbo build --filter=storybook",
    "build:packages": "turbo build --filter=./packages/*",
    "build:antd": "turbo build --filter=@raipiot-infra/antd",
    "build:utils": "turbo build --filter=@raipiot-infra/utils",
    "build:config": "turbo build --filter=@raipiot-infra/config",
    "preview:react": "turbo preview --filter=react-playground",
    "preview:next": "turbo preview --filter=next-playground",
    "test:hooks": "turbo test --filter=@raipiot-infra/hooks",
    "test:unit:hooks": "turbo test:unit --filter=@raipiot-infra/hooks",
    "test:coverage:hooks": "turbo test:coverage --filter=@raipiot-infra/hooks",
    "clean": "turbo clean && rimraf type-docs",
    "cspell:sort:dict": "turbo cspell:sort:dict --filter=@raipiot-infra/cspell",
    "type:doc": "typedoc",
    "cspell:check": "turbo cspell:check",
    "type:check": "turbo type:check",
    "prettier:check": "turbo prettier:check",
    "prettier:fix": "turbo prettier:fix",
    "eslint:check": "turbo eslint:check",
    "eslint:fix": "turbo eslint:fix",
    "cz": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release:generate": "pnpm changeset",
    "release:version": "pnpm changeset version",
    "release:publish": "pnpm build:packages && pnpm changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@raipiot-infra/commitlint-config": "workspace:^",
    "@raipiot-infra/cspell": "workspace:^",
    "@raipiot-infra/eslint-config": "workspace:^",
    "@raipiot-infra/eslint-config-ts": "workspace:^",
    "@raipiot-infra/prettier": "workspace:^",
    "@raipiot-infra/rpx": "workspace:^",
    "@raipiot-infra/tsconfig": "workspace:^",
    "@rushstack/eslint-patch": "^1.8.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cspell": "^8.6.0",
    "cz-git": "^1.9.0",
    "esbuild-sass-plugin": "^3.2.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "turbo": "^1.12.5",
    "typedoc": "^0.25.12",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "~5.3.3",
    "vite": "^5.1.5",
    "vitest": "^1.4.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "license": "MIT"
}
