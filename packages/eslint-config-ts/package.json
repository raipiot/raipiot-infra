{
  "name": "@raipiot-infra/eslint-config-ts",
  "version": "0.0.7",
  "description": "TypeScript ESLint config for raipiot",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "homepage": "https://github.com/raipiot/raipiot-infra#readme",
  "bugs": "https://github.com/raipiot/raipiot-infra/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/raipiot/raipiot-infra.git",
    "directory": "packages/eslint-config-ts"
  },
  "keywords": [
    "raipiot",
    "eslint",
    "eslint-config",
    "eslint-config-ts",
    "typescript"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "cspell:check": "cspell --no-progress --show-suggestions --show-context **",
    "eslint:check": "eslint . --color",
    "eslint:fix": "eslint . --color --fix",
    "prettier:check": "prettier --check --cache --ignore-unknown --ignore-path=../../.prettierignore .",
    "prettier:fix": "prettier --write --cache --ignore-unknown --ignore-path=../../.prettierignore ."
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@raipiot-infra/eslint-config-base": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
