{
  "name": "@raipiot-infra/antd",
  "version": "0.0.8",
  "description": "Collection of internal antd components.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "homepage": "https://github.com/raipiot/raipiot-infra#readme",
  "bugs": "https://github.com/raipiot/raipiot-infra/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/raipiot/raipiot-infra.git",
    "directory": "packages/antd"
  },
  "keywords": [
    "raipiot",
    "antd",
    "ui",
    "components"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist storybook-static",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "type:check": "tsc --pretty --noEmit",
    "eslint:check": "eslint . --color",
    "eslint:fix": "eslint . --color --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "antd": ">=5.13.3",
    "react": ">=18.2.0",
    "@raipiot-infra/enums": "workspace:*",
    "@raipiot-infra/utils": "workspace:*"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/test": "^7.6.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.52",
    "esbuild-sass-plugin": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
