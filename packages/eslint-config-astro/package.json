{
  "name": "@raipiot-infra/eslint-config-astro",
  "version": "0.0.2",
  "description": "React ESLint config for raipiot",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "homepage": "https://github.com/raipiot/raipiot-infra#readme",
  "bugs": "https://github.com/raipiot/raipiot-infra/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/raipiot/raipiot-infra.git",
    "directory": "packages/eslint-config-astro"
  },
  "keywords": [
    "raipiot",
    "eslint",
    "eslint-config",
    "eslint-config-astro",
    "astro",
    "typescript"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "eslint:check": "eslint . --color",
    "eslint:fix": "eslint . --color --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown ."
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@raipiot-infra/eslint-config-base": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.17.22",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.11.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-unused-imports": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
