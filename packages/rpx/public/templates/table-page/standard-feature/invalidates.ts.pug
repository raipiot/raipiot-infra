import { #{constNames}_QK, #{ccName}QK } from './query-keys'

export const invalidate#{pcNames}Queries = () =>
  queryClient.invalidateQueries({
    queryKey: [#{constNames}_QK]
  })


export const invalidate#{pcName}Query = (id: string) =>
  queryClient.invalidateQueries({
    queryKey: #{ccName}QK(id),
    refetchType: 'all',
  })

