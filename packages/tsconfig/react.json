{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "moduleResolution": "Node",
    "types": ["unplugin-icons/types/react"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src", "@types/**/*.d.ts"],
  "exclude": ["node_modules"],
  "references": [
    {
      "path": "./vite.json"
    }
  ]
}
