{
  "name": "@raipiot-infra/storybook",
  "version": "0.1.9",
  "description": "raipiot infra storybook",
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "clean": "rimraf dist storybook-static",
    "type:check": "tsc --pretty --noEmit",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "eslint:check": "eslint . --color --cache",
    "eslint:fix": "eslint . --color --cache --fix",
    "prettier:check": "prettier --check --cache --ignore-unknown --ignore-path=../../.prettierignore .",
    "prettier:fix": "prettier --write --cache --ignore-unknown --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@ant-design/happy-work-theme": "^1.0.0",
    "@raipiot-infra/antd": "workspace:^",
    "@raipiot-infra/enums": "workspace:^",
    "@raipiot-infra/theme": "workspace:^",
    "@raipiot-infra/utils": "workspace:^",
    "antd": "^5.15.0",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@raipiot-infra/tailwind": "workspace:^",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/builder-vite": "^7.6.17",
    "@storybook/preview-api": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@storybook/theming": "^7.6.17",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "autoprefixer": "^10.4.18",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "storybook": "^7.6.17",
    "storybook-dark-mode": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  },
  "private": true,
  "license": "MIT"
}
