{
  "name": "@raipiot-infra/react-playground",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "description": "raipiot infra React + Vite 演练场",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "type:check": "tsc --pretty --noEmit --composite false",
    "eslint:check": "eslint . --color --cache",
    "eslint:fix": "eslint . --color --cache --fix",
    "prettier:check": "prettier --check --cache --ignore-unknown --ignore-path=../../.prettierignore .",
    "prettier:fix": "prettier --write --cache --ignore-unknown --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@ant-design/happy-work-theme": "^1.0.0",
    "@lordicon/react": "^1.6.0",
    "@raipiot-infra/antd": "workspace:^",
    "@raipiot-infra/hooks": "workspace:^",
    "@raipiot-infra/utils": "workspace:^",
    "@raipiot-infra/axios": "workspace:^",
    "@tanstack/react-query": "^5.28.6",
    "@tanstack/react-router": "^1.21.0",
    "antd": "^5.15.3",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss-group-modifier-plugin": "^0.1.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.194",
    "@iconify/tailwind": "^0.1.4",
    "@raipiot-infra/bootstrap-animation": "workspace:^",
    "@raipiot-infra/eslint-config-react": "workspace:^",
    "@raipiot-infra/tailwind": "workspace:^",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@tanstack/router-devtools": "^1.21.0",
    "@tanstack/router-vite-plugin": "^1.20.5",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.1"
  },
  "private": true,
  "license": "MIT"
}
